{
	"name": "DT_PRQ_DYN",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_DLS",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"P_PATH": {
				"type": "string"
			},
			"P_CONTAINER": {
				"type": "string"
			},
			"P_FILENAME": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().P_FILENAME",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(\n    toUpper(dataset().P_PATH), \n    '/',\n    formatDateTime(utcNow(),'yyyy'), \n    '/', \n    formatDateTime(utcNow(),'MM'), \n    '/', \n    formatDateTime(utcNow(),'dd')    \n  \n)",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().P_CONTAINER",
					"type": "Expression"
				}
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}