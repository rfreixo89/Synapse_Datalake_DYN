						"parameters": {
							"P_PATH": {
								"value": "@pipeline().parameters.P_SYSTEM",
								"type": "Expression"
							},
							"P_CONTAINER": "landingzone",
							"P_FILENAME": {
								"value": "@pipeline().parameters.P_TABLENAME",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
