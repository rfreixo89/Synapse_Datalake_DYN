{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-event-trigger-dev"
		},
		"LS_SQL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQL'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=@{linkedService().P_FQDN_SRV};Initial Catalog=@{linkedService().P_DB_NAME}"
		},
		"AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0304-000144-5r6215d2"
		},
		"LS_DLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{concat('https://',\nlinkedService().P_STORAGE_ACCOUNT,\n'.dfs.core.windows.net')}"
		},
		"TRG_EVENT_LDZ_TO_RAW_properties_PL_EVENT_LDZ_RAW_parameters_P_PATH": {
			"type": "string",
			"defaultValue": "@triggerBody().folderPath"
		},
		"TRG_EVENT_LDZ_TO_RAW_properties_PL_EVENT_LDZ_RAW_parameters_P_FILENAME": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"TRG_EVENT_LDZ_TO_RAW_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/2bfc44c5-aef6-403f-bd57-d339da205622/resourceGroups/DL-Event-Trigger-dev/providers/Microsoft.Storage/storageAccounts/dlseventtriggerdev"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-event-trigger-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_SQL_connectionString": {
						"value": "[parameters('LS_SQL_connectionString')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					},
					"LS_DLS_properties_typeProperties_url": {
						"value": "[parameters('LS_DLS_properties_typeProperties_url')]"
					},
					"TRG_EVENT_LDZ_TO_RAW_properties_PL_EVENT_LDZ_RAW_parameters_P_PATH": {
						"value": "[parameters('TRG_EVENT_LDZ_TO_RAW_properties_PL_EVENT_LDZ_RAW_parameters_P_PATH')]"
					},
					"TRG_EVENT_LDZ_TO_RAW_properties_PL_EVENT_LDZ_RAW_parameters_P_FILENAME": {
						"value": "[parameters('TRG_EVENT_LDZ_TO_RAW_properties_PL_EVENT_LDZ_RAW_parameters_P_FILENAME')]"
					},
					"TRG_EVENT_LDZ_TO_RAW_properties_typeProperties_scope": {
						"value": "[parameters('TRG_EVENT_LDZ_TO_RAW_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}